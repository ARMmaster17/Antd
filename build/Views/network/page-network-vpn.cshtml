<legend id="VPN">VPN</legend>
<legend>VPN GRE/L2TP</legend>
<table class="table hovered">
    <thead>
        <tr>
            <th colspan="2">
                <label>Set VPN Options</label>
            </th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>
                <label>Default Route</label>
            </td>
            <td>
                <input id="NewVPNDefaultRoute" type="text" />
                <input type="button" id="UpdateVPNDefaultRoute" class="bg-anthilla-green" value="Apply" />
            </td>
        </tr>
        <tr>
            <td>
                <label>Server-side IP Range</label>
            </td>
            <td>
                <input id="NewVPNServerSideIP" type="text" />
                <input type="button" id="UpdateVPNServerSideIP" class="bg-anthilla-green" value="Apply" />
            </td>
        </tr>
        <tr>
            <td>
                <label>Client-side IP Range</label>
            </td>
            <td>
                <input id="NewVPNClientSideIP" type="text" />
                <input type="button" id="UpdateVPNClientSideIP" class="bg-anthilla-green" value="Apply" />
            </td>
        </tr>
        <tr>
            <td>
                <label>Server DNS</label>
            </td>
            <td>
                <input id="NewVPNServerDNS" type="text" />
                <input type="button" id="UpdateVPNServerDNS" class="bg-anthilla-green" value="Apply" />
            </td>
        </tr>
        <tr>
            <td>
                <label>Server WINS</label>
            </td>
            <td>
                <input id="NewVPNServerWINS" type="text" />
                <input type="button" id="UpdateVPNServerWINS" class="bg-anthilla-green" value="Apply" />
            </td>
        </tr>
    </tbody>
</table>
<br />
