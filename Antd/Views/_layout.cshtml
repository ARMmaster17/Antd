        <a href="/meminfo" class="element nav-button">Meminfo</a>
        <div class="element divider"></div>
        <a href="/cpuinfo" class="element nav-button">Cpuinfo</a>
        <div class="element divider"></div>
        <a href="/version" class="element nav-button">Version</a>
        <div class="element divider"></div>
        <a href="/command" class="element nav-button">Command</a>
        <div class="element divider"></div>
    </nav>
</nav>
    @RenderSection("MainContent", required: false)

    @RenderSection("Scripts", required: false)
